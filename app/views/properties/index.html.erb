<div class="row">
	<div class="col-lg-12">
		<div class="box">
			<div class="row">
        <div class="col-lg-12">
          <section class="panel">
            <div class="panel-body">
              <form class="form-inline" role="form">
                <div class="form-group">
                  <label class="sr-only" for="exampleInputEmail2">Min Sqft</label>
                  <input type="email" class="form-control" id="exampleInputEmail2" placeholder="Min Sqft">
                </div>
                <div class="form-group">
                  <label class="sr-only" for="exampleInputPassword2">Max Sqft</label>
                  <input type="password" class="form-control" id="exampleInputPassword2" placeholder="Max Sqft">
                </div>
                <button type="submit" class="btn btn-success">Search</button>
              </form>
            </div>
          </section>
        </div>
      </div>
			<div class="box-header" data-original-title>
				<h2><i class="icon-user"></i><span class="break"></span>2,391 office spaces for rent </h2>
				<div class="box-icon">
					<a href="table.html#" class="btn-setting"><i class="icon-wrench"></i></a>
					<a href="table.html#" class="btn-minimize"><i class="icon-chevron-up"></i></a>
					<a href="table.html#" class="btn-close"><i class="icon-remove"></i></a>
				</div>
			</div>
			<div class="box-content">
				<table class="table table-striped table-bordered bootstrap-datatable datatable">
				  <thead>
					  <tr>
						  <th>Id</th>
						  <th>Photos</th>
						  <th>Project</th>
						  <th>Area</th>
						  <th>City</th>
						  <th>Posted</th>
						  <th>Price</th>
						  <th>Sizes</th>
						  <th>For Sale/Buy/Rent</th>
						  <th>Contact</th>
					  </tr>
				  </thead>
				  <tbody>
				  	<% @properties.each do |p|  %>
						<tr>
							<td><%= p.id  %></td>
							<td class="center"></td>
							<td class="center"><%= p.project %></td>
							<td class="center"><%= p.area %></td>
							<td class="center"><%= p.city %></td>
							<td>4th Feb</td>
							<td class="center"><%= p.price %></td>
							<td class="center"><%= p.sizes %></td>
							<td class="center">
								<span class="label label-success">Rent</span>
							</td>
							<td class="center"><%= p.contact %></td>
							</td>
						</tr>
					<% end %>
				 </tbody>
			  </table>
			</div>
		</div>
	</div><!--/col-->

</div><!--/row-->