<div class="col-md-4">
  <div class="panel panel-primary">
    <div class="panel-heading">
      <h3 style="margin-top:0px;">Intrested?</h3><h3 class="panel-title">Get in Touch with the owner</h3>
    </div>
    <div class="panel-body">

      <%= form_tag(send_query_properties_path, {:method => :get, :class => "form form-horizontal"}) do %>
        <div class="form-group">
          <div class="col-md-12"><%= text_field_tag "email", params[:email], :class => "form-control", :placeholder => "Email" %></div>
        </div>
        <div class="form-group">
          <div class="col-md-12"><%= text_field_tag "name", params[:name], :class => "form-control", :placeholder => "Name" %></div>
        </div>
        <div class="form-group">
          <div class="col-md-12"><%= text_field_tag "phone", params[:phone], :placeholder => "Mobile No.", :class => "form-control datetime" %></div>
        </div>
        <div class="form-actions form-actions-padding-sm">
          <div class="row">
            <div class="col-md-12">
              <%= submit_tag "Send Message",id: "do_migration", :class => "pull-right btn btn-warning" %>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>